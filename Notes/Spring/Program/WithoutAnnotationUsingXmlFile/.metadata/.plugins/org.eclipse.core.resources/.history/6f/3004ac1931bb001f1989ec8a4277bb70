package org.jsp;

public class Student {

	int id ;
	String name ;
	double marks ;
	
	public void settId(int id) {
		this.id = id ;
	}
	
	public void setName(String name) {
		this.name = name ;
	}
	
	public void setMarks(double marks) {
		this.marks = marks ;
	}
	
	public double getMarks() {
		return marks ;
	}
	
	public int getId() {
		return id ;
	}
	
	public String getName() {
		return name ;
	}
}
